{
  "name": "vkim",
  "version": "0.0.1",
  "description": "VK Improved. Some improvements for vk.com",
  "manifest_version": 2,
  "permissions": [
    "http://*/", "https://*/", "tabs"
  ],  
  "content_scripts": [
    {
      "matches": ["http://vk.com/*", "http://vkontakte.ru/*"],
      "js" : ["contentscript.js"],
      "css": ["style.css"]
    }
  ]
}
